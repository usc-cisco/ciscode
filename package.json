{
  "name": "ciscode",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -- next dev --turbopack",
    "build": "next build",
    "start": "dotenv -- next start",
    "lint": "next lint",
    "db:sync": "bun build src/db/sync.ts --target=node --outfile=/tmp/sync.mjs && node /tmp/sync.mjs",
    "db:seed": "bun build src/db/seed.ts --target=node --outfile=/tmp/seed.mjs && node /tmp/seed.mjs",
    "db:reset": "bun build src/db/sync.ts --target=node --outfile=/tmp/sync.mjs && node /tmp/sync.mjs && bun build src/db/seed.ts --target=node --outfile=/tmp/seed.mjs && node /tmp/seed.mjs",
    "prepare": "husky install"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@uiw/react-markdown-preview": "^5.1.5",
    "@uiw/react-md-editor": "^4.0.8",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "devicons-react": "^1.4.1",
    "diff": "^8.0.2",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "katex": "^0.16.22",
    "lint-staged": "^16.1.5",
    "lucide-react": "^0.525.0",
    "mysql2": "^3.14.2",
    "next": "15.4.3",
    "next-themes": "^0.4.6",
    "node-pty": "^1.0.0",
    "pg-hstore": "^2.3.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "react-split": "^2.0.14",
    "rehype-katex": "^7.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/katex": "^0.16.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
